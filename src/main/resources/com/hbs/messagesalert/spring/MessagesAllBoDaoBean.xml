<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="messagesBo" class="com.hbs.messagesalert.bo.impl.MessagesBoImpl">
		<property name="messagesDAO" ref="messagesDAO"></property>
		<property name="userBo" ref="userBo"></property>
	</bean>
	<bean id="messagesDAO" class="com.hbs.messagesalert.dao.impl.MessagesDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
</beans>