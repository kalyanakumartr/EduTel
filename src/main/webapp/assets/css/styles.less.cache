  text-align: left;
  padding-left: 2px;
}
.countDays:after,
.countHours:after,
.countMinutes:after,
.countSeconds:after {
  content: '';
  display: block;
  position: absolute;
  width: 100%;
  height: 50%;
  z-index: 10;
  top: 0;
  left: 0;
  background-color: #3b4448;
  border-top-left-radius: 2px;
  border-top-right-radius: 2px;
}
